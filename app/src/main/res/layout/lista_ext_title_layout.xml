<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ListaProcesos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".listview.ListaProcesosExport"
    android:orientation="horizontal"
    android:background="@color/White"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:divider="@android:color/transparent"
    android:dividerHeight="10dp"
    android:scrollbars="none"

    >

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_weight="1">
        <!-- request button -->
        <!--CONTENT/////////////////////////////////////////////////-->

        <TextView
            android:id="@+id/TituloListExt"
            android:layout_width="95dp"
            android:layout_height="60dp"
            android:padding="10dp"
            android:textAlignment="center"
            android:textColor="#f5f9f5"
            android:textSize="15sp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="19dp" />
       
    </RelativeLayout>

        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="80dp"
            android:id="@+id/relative"
            android:textAlignment="center"
            android:layout_alignTop="@+id/liberadoList"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true">

            <TextView
                android:id="@+id/contenidoListArriba"
                android:layout_width="280dp"
                android:layout_height="40dp"
                android:padding="0dp"
                android:textColor="@color/mainTextColor"
                android:textSize="15sp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="16dp"/>

            <TextView
                android:id="@+id/contenidoListAbajo"
                android:layout_width="280dp"
                android:layout_height="40dp"
                android:padding="0dp"
                android:textColor="@color/mainTextColor"
                android:textSize="15sp"
                android:layout_below="@+id/contenidoListArriba"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
        </RelativeLayout>




</LinearLayout>
